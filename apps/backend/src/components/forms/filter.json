{
  "collectionName": "components_forms_filters",
  "info": {
    "displayName": "Filter",
    "icon": "collapse",
    "description": ""
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "sort": {
      "type": "customField",
      "options": [
        "Hight to Low:price-desc",
        "Low to High:price-asc",
        "A to Z:title-desc",
        "Z to A:title-asc",
        "New to Old:date-desc",
        "Old to New:date-asc"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "jobType": {
      "type": "customField",
      "options": [
        "Full Time:Full Time",
        "Part Time:Part Time",
        "Freelance:Freelance",
        "Internship:Internship",
        "Contract Based:Contract Based"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "company_size": {
      "type": "customField",
      "options": ["1-24", "25-49", "50-99", "100-199", "200-500"],
      "customField": "plugin::multi-select.multi-select"
    },
    "avarage_salary": {
      "type": "customField",
      "options": ["10k-20k", "21k-30k", "31k-50k", "51k-100k"],
      "customField": "plugin::multi-select.multi-select"
    },
    "revenue": {
      "type": "customField",
      "options": ["0-50,000", "50,001-100,000"],
      "customField": "plugin::multi-select.multi-select"
    },
    "rate": {
      "type": "customField",
      "options": ["10k", "20k", "30k", "40k"],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
