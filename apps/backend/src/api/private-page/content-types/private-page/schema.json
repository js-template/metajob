{
  "kind": "collectionType",
  "collectionName": "private_pages",
  "info": {
    "singularName": "private-page",
    "pluralName": "private-pages",
    "displayName": "Private Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "role": {
      "type": "enumeration",
      "enum": [
        "Candidate",
        "Employer"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "config.message",
        "config.header-config",
        "config.recent-jobs-header",
        "table.bookmark",
        "table.meta",
        "table.pricing",
        "widget.total-list",
        "widget.open-list",
        "widget.closed-list",
        "shared.spacing",
        "block.bookmark-list",
        "block.recent-activities",
        "widget.applied-lists",
        "widget.matched-lists",
        "widget.favorite-lists",
        "block.latest-applied",
        "block.manage-lists",
        "block.manage-companies",
        "table.applied-jobs",
        "block.manage-resume"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "styles": {
      "displayName": "gird container",
      "type": "component",
      "repeatable": false,
      "component": "component.gird-container",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "permissions": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": [
        "candidate",
        "employer"
      ],
      "customField": "plugin::multi-select.multi-select",
      "required": true
    },
    "page_url": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo"
    }
  }
}
