{
  "kind": "collectionType",
  "collectionName": "private_pages",
  "info": {
    "singularName": "private-page",
    "pluralName": "private-pages",
    "displayName": "Private Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "role": {
      "type": "enumeration",
      "enum": ["Candidate", "Employer"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },

    "styles": {
      "displayName": "grid container",
      "type": "component",
      "repeatable": false,
      "component": "component.grid-container",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "page_url": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo"
    }
  }
}
